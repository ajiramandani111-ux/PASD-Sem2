public class MahasiswaMain15 {
    public static void main(String[] args) {

        Mahasiswa15 mhs1 = new Mahasiswa15();

        mhs1.nim = "15";
        mhs1.nama = "Muhamad Aji Ramandani";
        mhs1.kelas = "SIB 1E";
        mhs1.ipk = 3.25;

        mhs1.tampilkanInformasi();

        mhs1.ubahKelas("SIB 1E");
        mhs1.updateIpk(3.60);

        mhs1.tampilkanInformasi();

    }
}
